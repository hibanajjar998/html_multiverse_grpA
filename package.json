    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "jquery": "^3.6.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": ["./jest.setup.js"]
  }
}